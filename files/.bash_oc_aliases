## oc functions
####################

# Get / set context
occ ()
{
    if [ -z $1 ]; then
        oc config get-contexts;
    else
        oc config use-context $1;
    fi
}

# Switch namespace
ocn()
{
    if [ -z $1 ]; then
        mapfile -t namespaces < <(oc get projects --no-headers -o custom-columns=":metadata.name");
        i=0;
        for n in ${namespaces[@]};
        do
            echo "$i) $n";
            (( i++ ));
        done;
        echo -n "Choose project: ";
        read r;
        ns=${namespaces[$r]};
        echo "Set project to: $ns";
    else
        ns=$1;
    fi;
    oc project $ns
    PS1="[$(fgc ${OC_NAME}/${ns} $(ocolor $OC_NAME))]\n$PPS1"

}
alias opr='ocn'

# Exec shell in pod
oexe()
{
    oc exec -ti $@ -- /bin/bash || oc exec -ti $@ -- /bin/sh
}

# Exec shell in pod in namespace
oexen()
{
    oc exec -ti $1 --namespace $2 -- /bin/bash || oc exec -ti $1 --namespace $2 -- /bin/sh
}

# Show secret contents
ogsedata() {
    oc get secret $1 -o go-template --template '{{range $key, $data := .data}}{{printf "%s %s\n" $key $data }}{{end}}' | while read k d; do echo $k:; echo $d | base64 -d; echo; done
}

# Find toolbox pod and exec shell in it
otb()
{
    TOOLBOX_POD=$(oc get pods --selector=app=toolbox --all-namespaces -o jsonpath='{.items[0].metadata.name}');
    TOOLBOX_NS=$(oc get pods --selector=app=toolbox --all-namespaces -o jsonpath='{.items[0].metadata.namespace}');
    oc exec -ti -n $TOOLBOX_NS $TOOLBOX_POD -- /bin/bash
}

# Get color for specific OpenShift
ocolor() {
  case $1 in
      'ocd')
          echo 117
          ;;
      'ocp')
          echo 202
          ;;
      'oce')
          echo 227
          ;;
  esac
  echo 231
}

# Login to cluster
#   Log to OCD as admin: olog d qs
olog() {
    export KUBECONFIG=$HOME/.kube/config_oc${1}
    if [ -n "$(_getcred ${2}user)" ]; then
        user="-u $(_getcred ${2}user)"
    else
        user=""
    fi
    if [ -n "$(_getcred ${2}password)" ]; then
        password="-p $(_getcred ${2}password)"
    else
        password=""
    fi

    if oc login https://api.oc${1}.cc.corp:6443 $user $password ; then
        OC_NAME=oc${1}
        [ -z "$PPS1" ] && PPS1=$PS1 
        OC_CTX=$(oc config current-context)
        PS1="[$(fgc ${OC_NAME}/${OC_CTX%%/*} $(ocolor $OC_NAME))]\n$PPS1"
    fi
}

ologout() {
    oc logout && unset OC_NAME;
    PS1=$PPS1
}

# Search for DevWorkspace
odev() {
    for ns in $(oc get project -o "custom-columns=NAME:metadata.name" --no-headers=true); do
        if [[ $(oc get devworkspace -n $ns -o go-template='{{len .items}}' 2>/dev/null) -gt 0 ]]; then
            echo "Namespace: $ns";
            oc get devworkspace -n $ns;
        fi
    done
}

alias ologd='olog d'
alias ologp='olog p'

# OpenShift
## Kubecolor
export KUBECTL_COMMAND="oc"
alias oc='kubecolor'
alias ooc='~/.local/bin/oc'

# Bash completion
_opods() {
    local cur prev opts
    COMPREPLY=()
    if [[ -n "$OC_NAME" ]]; then
        cur="${COMP_WORDS[COMP_CWORD]}"
        opts=$(oc get pods -o jsonpath='{.items[*].metadata.name}')

        COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
    fi
}

_odeployments() {
    local cur prev opts
    COMPREPLY=()
    if [[ -n "$OC_NAME" ]]; then
        cur="${COMP_WORDS[COMP_CWORD]}"
        opts=$(oc get deployments -o jsonpath='{.items[*].metadata.name}')

        COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
    fi
}

complete -F _opods oexe
complete -F _opods ormp
complete -F _opods ogp
complete -F _opods olo
complete -F _odeployments ormde
complete -F _odeployments ogde
